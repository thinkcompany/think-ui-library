{
  "name": "@thinkcompany/think-ui-library",
  "version": "1.4.50",
  "repository": {
    "type": "git",
    "url": "https://github.com/thinkcompany/think-ui-library"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/thinkcompany"
  },
  "description": "Pattern Library for the Think Company Website",
  "main": "index.js",
  "scripts": {
    "start": "npm run storybook",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "tokens": "node ./build/scripts/tokens.js && npm run format",
    "icons": "node ./build/scripts/combine-svg.js",
    "build": "NODE_ENV=production webpack --config build/config/webpack.config.babel.js",
    "lint": "npm run lint:sass && npm run lint:js",
    "lint:sass": "stylelint '**/*.scss' --fix",
    "lint:js": "eslint \"src/**/*.{js,jsx}\"",
    "format": "prettier --config ./prettier.config.js --ignore-path ./.prettierignore \"src/**/*.{js,json,scss,css}\" --write",
    "ci": "npm install",
    "chromatic": "npx chromatic --project-token=8e2767946d07 --exit-zero-on-changes",
    "prepublishOnly": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "",
