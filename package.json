    "fs-extra": "^9.0.1",
    "globals": "^15.9.0",
    "html-webpack-plugin": "^4.5.1",
    "husky": "^4.3.7",
    "lint-staged": "^11.0.0",
    "mini-css-extract-plugin": "^1.6.0",
    "prettier": "^3.3.3",
