{
  "name": "@thinkcompany/think-ui-library",
  "version": "1.2.139",
  "repository": {
    "type": "git",
    "url": "https://github.com/thinkcompany/think-ui-library"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/thinkcompany"
  },
  "description": "Pattern Library for the Think Company Website",
  "main": "index.js",
  "scripts": {
    "start": "npm run storybook",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -p 6006 -s ./src/assets ./src",
    "build-storybook": "build-storybook -s ./src/assets",
    "tokens": "node ./build/scripts/tokens.js && npm run format",
    "icons": "node ./build/scripts/combine-svg.js",
    "build": "webpack -p --config build/config/webpack.config.babel.js",
    "lint": "npm run lint:sass && npm run lint:js",
    "lint:sass": "stylelint '**/*.scss' --fix",
    "lint:js": "eslint 'src/**/*.js'",
    "format": "prettier --config ./prettier.config.js --ignore-path ./.prettierignore \"src/**/*.{js,json,scss,css}\" --write",
    "ci": "npm install",
    "prepublishOnly": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "",
  "dependencies": {
    "css-loader": "^3.6.0",
    "eslint-plugin-prettier": "^3.3.1",
    "normalize.css": "^8.0.1",
    "sass": "1.25.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "@stormid/storybook-html-addon-markup": "^0.1.1",
    "@storybook/addon-a11y": "^6.1.11",
    "@storybook/addon-knobs": "^6.1.11",
    "@storybook/addon-viewport": "^6.1.11",
    "@storybook/html": "^6.1.11",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.4.1",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.4",
    "file-loader": "^6.2.0",
    "fs-extra": "^9.0.1",
    "html-webpack-plugin": "^4.5.1",
    "husky": "^4.3.7",
    "lint-staged": "^10.4.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^7.0.35",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.2.1",
    "stylelint": "^13.8.0",
    "stylelint-config-hudochenkov": "^5.0.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "stylelint-webpack-plugin": "^1.2.3",
    "svg-sprite": "^1.5.0",
    "terser-webpack-plugin": "^2.3.5",
    "webpack": "^4.45.0",
    "webpack-cli": "^3.3.12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,scss}": [
      "prettier --config ./prettier.config.js --ignore-path ./.prettierignore --write"
    ],
    "*.scss": [
      "stylelint --syntax=scss --fix"
    ],
    "*.{js}": [
      "eslint --ext .js.,jsx --fix"
    ]
  }
}
