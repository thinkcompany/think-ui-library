    "lite-youtube-embed": "^0.3.2",
    "normalize.css": "^8.0.1",
    "sass-loader": "^11.1.1",
    "save-dev": "^0.0.1-security",
    "style-loader": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
