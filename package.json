    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "@chromatic-com/storybook": "^1.9.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.11.0",
    "@stormid/storybook-html-addon-markup": "^0.1.1",
    "@storybook/addon-a11y": "^8.3.4",
    "@storybook/addon-backgrounds": "^8.3.4",
    "@storybook/addon-docs": "^8.3.4",
    "@storybook/addon-essentials": "^8.3.4",
    "@storybook/addon-mdx-gfm": "^8.3.4",
    "@storybook/addon-viewport": "^8.3.4",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.3",
    "@storybook/html": "^8.3.4",
    "@storybook/html-webpack5": "^8.3.4",
    "@storybook/theming": "^8.3.4",
    "autoprefixer": "^10.2.6",
    "babel-eslint": "^10.1.0",
